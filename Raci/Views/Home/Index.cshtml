@section scripts
{
    <script type="text/javascript">
        $(function () {
            $(".submit-button").click(function () {
                window.location.href = "http://raci.azurewebsites.net/?original=" + $(".original").val() + "&blocks=" + $(".blocks").val();
            });
        });

    </script>
}
