@model Raci.Controllers.FlickModel

<div id="body">
    <section class="featured">
        
    </section>
    <section class="content-wrapper main-content clear-fix">
@*        <img class="image" src="@Model.BaseUrl" alt="weeeeee"/>
        <img class="image2" src="@Model.BlockUrl" alt="weeeeee"/>
        <img class="image3" src="@Model.FinishedUrl" alt="weeeeee"/>*@
        <img src="data:image/png;base64,@Model.BaseEncoded" />
        <img src="data:image/png;base64,@Model.BlockEncoded" />
